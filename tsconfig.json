{
  "compilerOptions": {
    "esModuleInterop": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "lib": ["DOM", "webworker"],
    "module": "ES2020",
    "moduleResolution": "node",
    "paths": {
      "~/*": ["./src/*"]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es6"
  }
}
